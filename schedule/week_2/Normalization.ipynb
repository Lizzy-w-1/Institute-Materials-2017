    "    return {\"id\": siglum, \"tokens\": \\\n",
    "            [processToken(token) for token in re.findall(r'\\S+\\s*', rdg)]}\n",
    "witnesses = [('A', 'Yes, no, or maybe'), ('B', 'Yes; no; or maybe')]\n",
